{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:dev": "NODE_ENV=development nuxt build",
    "build:staging": "NODE_ENV=staging nuxt build",
    "build:prod": "NODE_ENV=production nuxt build",
    "dev": "nuxt dev",
    "dev:staging": "NODE_ENV=staging nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nuxt typecheck",
    "test": "echo \"No tests specified\" && exit 0",
    "test:ci": "npm run format:check && npm run build",
    "deploy:dev": "npm run build:dev && echo \"Deploy to development\"",
    "deploy:staging": "npm run build:staging && echo \"Deploy to staging\"",
    "deploy:prod": "npm run build:prod && echo \"Deploy to production\""
  },
  "dependencies": {
    "nuxt": "^4.0.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nuxt/typescript-build": "^3.0.2",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^15.12.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
